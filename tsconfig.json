{
  "compilerOptions": {
    "target": "es5",
    "module": "ES6",
    "noEmit": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "allowJs": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "alwaysStrict": true
  }
}