{
  "compilerOptions": {
    "target": "es2017",
    "module": "CommonJS",
    "noEmit": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "alwaysStrict": true
  }
}